<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AIIntegrator.AISortContentView">

    <Grid x:Name="myGrid">
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <ActivityIndicator x:Name="myInActivityIndicator" Color="Orange" IsRunning="true" Grid.Row="0" />


        <WebView x:Name="WebviewAiSort" Grid.Row="0" />



        <StackLayout Orientation="Horizontal" Grid.Row="1" HorizontalOptions="CenterAndExpand">
            <Button Text="保存设置" WidthRequest="200" Clicked="Button_Clicked" />
            <Button Text="取消设置" WidthRequest="200" Clicked="Button_Clicked_1" />
        </StackLayout>
    </Grid>




</ContentView>
